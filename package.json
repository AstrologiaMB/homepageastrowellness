{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "prisma migrate deploy && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "gen:api:calendar": "openapi --input ./lib/api-clients/calendar-spec.json --output ./lib/api-clients/calendar --client fetch --name CalendarClient",
    "gen:api:natal-chart": "openapi --input ./lib/api-clients/natal-chart-spec.json --output ./lib/api-clients/natal-chart --client fetch --name NatalChartClient",
    "gen:api:astrogematria": "openapi --input ./lib/api-clients/astrogematria-spec.json --output ./lib/api-clients/astrogematria --client fetch --name AstrogematriaClient",
    "gen:api:carta-electiva": "openapi --input ./lib/api-clients/carta-electiva-spec.json --output ./lib/api-clients/carta-electiva --client fetch --name CartaElectivaClient",
    "gen:api:astro-interpreter": "openapi --input ./lib/api-clients/astro-interpretador-spec.json --output ./lib/api-clients/astro-interpreter --client fetch --name AstroInterpreterClient",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest tests/unit",
    "test:integration": "vitest tests/integration",
    "test:e2e": "vitest tests/e2e",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "clean": "rm -rf .next node_modules/.cache",
    "analyze": "ANALYZE=true npm run build",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@astrodraw/astrochart": "^3.0.2",
    "@aws-sdk/client-ses": "^3.879.0",
    "@hookform/resolvers": "^3.9.1",
    "@prisma/client": "^6.7.0",
    "@react-google-maps/api": "^2.20.8",
    "@sentry/nextjs": "^10.38.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jspdf": "^1.3.3",
    "@vis.gl/react-google-maps": "^1.7.1",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "3.6.0",
    "embla-carousel-react": "8.5.1",
    "html2canvas": "^1.4.1",
    "input-otp": "1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.562.0",
    "micro": "^10.0.1",
    "next": "15.2.4",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.4",
    "pdf-lib": "^1.17.1",
    "radix-ui": "^1.4.3",
    "react": "^19",
    "react-day-picker": "^9.0.0",
    "react-dom": "^19",
    "react-hook-form": "^7.56.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "2.15.0",
    "resend": "^6.4.2",
    "sharp": "^0.33.5",
    "sonner": "^1.7.1",
    "stripe": "^20.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tz-lookup": "^6.1.25",
    "vaul": "^1.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.1.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/tz-lookup": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
