    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.0",
    "openapi-typescript-codegen": "^0.30.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prisma": "^6.7.0",
    "shadcn": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}