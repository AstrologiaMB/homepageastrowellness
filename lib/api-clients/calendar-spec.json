{
    "openapi": "3.1.0",
    "info": {
        "title": "Personal Astrology Calendar API",
        "description": "Complete microservice for calculating personal astrological events: transits, lunar phases, eclipses, progressed moon, and profections",
        "version": "2.0.0"
    },
    "paths": {
        "/calculate-personal-calendar-dynamic": {
            "post": {
                "summary": "Calculate Personal Calendar Dynamic Endpoint",
                "description": "Calculate personal astrological calendar events using dynamic natal chart calculation.\nThis endpoint receives basic birth data and calculates the complete natal chart dynamically.\nReturns Strict Types (datetime, etc).",
                "operationId": "calculate_personal_calendar_dynamic_endpoint_calculate_personal_calendar_dynamic_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/BirthDataRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CalculationResponseStrict"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/calculate-personal-calendar": {
            "post": {
                "summary": "Calculate Personal Calendar Endpoint",
                "description": "Legacy endpoint: Calculate personal astrological calendar events using pre-calculated natal chart.\nThis endpoint receives a complete natal chart and uses it for calculations.",
                "operationId": "calculate_personal_calendar_endpoint_calculate_personal_calendar_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/NatalDataRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CalculationResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/calculate-cycles": {
            "post": {
                "summary": "Calculate Cycles Endpoint",
                "description": "Identify active Lunar Gestation Cycles (Families) for a specific date.\nTraces back from the target date (if it's a key phase) to the Seed New Moon,\nand returns the full 27-month cycle timeline.\nAlso calculates Metonic Index based on birth date.",
                "operationId": "calculate_cycles_endpoint_calculate_cycles_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CycleAnalysisRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ActiveCyclesResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "summary": "Health Check",
                "description": "Health check endpoint.",
                "operationId": "health_check_health_get",
                "responses": {
